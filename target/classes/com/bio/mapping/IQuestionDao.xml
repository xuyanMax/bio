<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bio.dao.IQDao">
    <insert id="insertQuestion">
        insert into questionnaire (idperson, filling_time, qtnaire_version, score) values (#{idperson}, #{filling_time}, #{qtnaire_version}, #{score});
    </insert>
    <select id="selectQuestion" resultType="com.bio.beans.Question">
        select * from questionnaire where filling_time=#{xxx}
    </select>
</mapper>