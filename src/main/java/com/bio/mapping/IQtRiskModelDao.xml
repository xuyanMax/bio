<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bio.dao.IQtRiskModelDao">

    <select id="selectRiskModelByVersion" resultType="com.bio.beans.Qtnaireversion_riskmodel">
        select * from qtnaireversion_riskmodel where qtnaire_version=#{###};
	</select>

    <select id="selectRiskModelByVersionLimitOne" resultType="com.bio.beans.Qtnaireversion_riskmodel">
        select * from qtnaireversion_riskmodel where qtnaire_version=#{###} limit 1;
	</select>

</mapper>
